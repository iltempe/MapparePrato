<script type="text/javascript">
function setCookie(){
   var date = new Date();
   date.setTime(date.getTime() + (365 * 24 * 60 * 60 * 1000));
   var expires = "; expires=" + date.toGMTString();
   document.cookie = "CookiesSirRegioneToscana=enable" + expires + "; path=/";
   document.getElementById('myCookie').style.visibility = "hidden";
}

function getCookie(name){
   var value = "; " + document.cookie;
   var parts = value.split("; " + name + "=");
   if (parts.length == 2) return parts.pop().split(";").shift();
}

if(getCookie('CookiesSirRegioneToscana') != 'enable'){
   document.getElementById('myCookie').style.visibility = "visible";
